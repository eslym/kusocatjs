{
  "name": "@kusocat/inertia-svelte",
  "version": "1.0.0-beta.0",
  "type": "module",
  "scripts": {
    "build": "bun run build.ts"
  },
  "files": [
    "src/**/*.ts",
    "src/**/*.svelte",
    "dist/**/*.ts",
    "dist/**/*.svelte",
    "types.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts",
      "svelte": "./dist/index.js"
    },
    "./ssr": {
      "import": "./dist/ssr.js",
      "types": "./src/ssr.ts",
      "svelte": "./dist/ssr.js"
    }
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.isequal": "^4.5.6",
    "bun-types": "^1.0.4",
    "glob": "^10.3.10"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@inertiajs/core": "^1.0.11",
    "lodash.isequal": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "svelte": "^3 || ^4"
  }
}
